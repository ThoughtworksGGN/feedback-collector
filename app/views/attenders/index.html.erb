<h1>Listing attenders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Company</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @attenders.each do |attender| %>
      <tr>
        <td><%= attender.name %></td>
        <td><%= attender.email %></td>
        <td><%= attender.phone %></td>
        <td><%= attender.company %></td>
        <td><%= link_to 'Show', attender %></td>
        <td><%= link_to 'Edit', edit_attender_path(attender) %></td>
        <td><%= link_to 'Destroy', attender, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attender', new_attender_path %>
